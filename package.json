{
  "name": "remix-auth-starter",
  "private": true,
  "description": "Add sign up, sign in, profile management, and authenticated API routes to your Remix application with the Remix Auth Starter by Clerk.",
  "keywords": [
    "remix",
    "auth",
    "starter",
    "clerk",
    "react"
  ],
  "author": "Clerk, Inc.",
  "license": "MIT",
  "homepage": "https://github.com/clerkinc/remix-auth-starter",
  "repository": {
    "type": "git",
    "url": "https://github.com/clerkinc/remix-auth-starter"
  },
  "scripts": {
    "build": "npm run build:css && cross-env NODE_ENV=production remix build",
    "build:css": "tailwindcss -m -i ./styles/app.css -o app/styles/app.css",
    "dev": "concurrently \"npm run dev:css\" \"cross-env NODE_ENV=development remix dev\"",
    "dev:css": "tailwindcss -w -i ./styles/app.css -o app/styles/app.css"
  },
  "dependencies": {
    "@clerk/remix": "^2.0.5",
    "@headlessui/react": "^1.7.8",
    "@heroicons/react": "^2.0.14",
    "@remix-run/node": "^1.3.5",
    "@remix-run/react": "^1.3.5",
    "@remix-run/vercel": "^1.3.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.3.5",
    "@remix-run/eslint-config": "^1.3.5",
    "@remix-run/serve": "^1.3.5",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "autoprefixer": "^10.4.13",
    "concurrently": "^7.6.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.9.0",
    "postcss": "^8.4.21",
    "prettier": "^2.5.1",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.6.2"
  },
  "engines": {
    "node": ">=14"
  }
}
