{
  "name": "zero-remix-frontend",
  "private": true,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jalexanderII/zero-remix-frontend"
  },
  "sideEffects": false,
  "scripts": {
    "build": "npm run build:css && cross-env NODE_ENV=production remix build",
    "build:css": "tailwindcss -m -i ./styles/app.css -o app/styles/app.css",
    "dev": "concurrently \"npm run dev:css\" \"cross-env NODE_ENV=development remix dev\"",
    "dev:css": "tailwindcss -w -i ./styles/app.css -o app/styles/app.css",
    "typecheck": "tsc",
    "prepare": "husky install"
  },
  "prettier": {},
  "eslintIgnore": [
    "/node_modules",
    "/build",
    "/public/build"
  ],
  "dependencies": {
    "@babel/core": ">=7.0.0 <8.0.0",
    "@clerk/remix": "^2.1.1",
    "@emotion/cache": "^11.10.5",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@fontsource/roboto": "^4.5.8",
    "@headlessui/react": "^1.7.8",
    "@heroicons/react": "^2.0.14",
    "@mui/icons-material": "^5.11.0",
    "@mui/material": "^5.11.8",
    "@remix-run/node": "^1.12.0",
    "@remix-run/react": "^1.12.0",
    "@remix-run/server-runtime": "^1.7.6",
    "@remix-run/vercel": "^1.12.0",
    "@tremor/react": "^1.6.1",
    "@vercel/analytics": "^0.1.9",
    "axios": "^1.3.2",
    "isbot": "^3.6.5",
    "material-react-table": "^1.7.4",
    "prop-types": ">=15.6.0 <16.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-table": "^7.8.0",
    "web-vitals": "^3.1.1",
    "zustand": "^4.3.3"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.12.0",
    "@remix-run/eslint-config": "^1.12.0",
    "@remix-run/serve": "^1.12.0",
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@types/react-table": "^7.7.14",
    "@vercel/node": "^2.9.0",
    "autoprefixer": "^10.4.13",
    "concurrently": "^7.6.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.33.0",
    "husky": "^8.0.0",
    "lint-staged": "^13.1.2",
    "postcss": "^8.4.21",
    "prettier": "2.8.3",
    "tailwindcss": "^3.2.4",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "engines": {
    "node": ">=14"
  },
  "eslintConfig": {
    "extends": [
      "@remix-run/eslint-config",
      "@remix-run/eslint-config/node"
    ]
  },
  "lint-staged": {
    "*.{ts,jsx,tsx}!(*config).js": "eslint --cache --fix",
    "*.{!(*config).js,ts,jsx,tsx,cdd,md}": "prettier --write --ignore-unknown"
  }
}
